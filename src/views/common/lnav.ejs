<script>
	document.addEventListener('DOMContentLoaded', function () {
		const nav = document.querySelector('.header[data-adBanner]')
		if (window.matchMedia("(max-width: 450px)").matches) {
			function fixdiv() {
				let getBannerHeight = document.querySelector('.sticky-ads-1').offsetHeight;
				nav.style.bottom = `${getBannerHeight}px`;
			}
			window.addEventListener('resize', function () {
				fixdiv()
			})
			fixdiv()
		}
		if (document.getElementById('closeAdBanner') !== null) {
			const closeAdBanner = document.getElementById('closeAdBanner');
			closeAdBanner.addEventListener('click', function (event) {
				event.preventDefault();
				closeAdBanner.closest('.ft-banner').classList.add('hide');
				nav.style.bottom = `${0}px`;
			})
		}
	})
</script>

