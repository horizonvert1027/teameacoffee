<% function shuffle(a) { for (let i=a.length - 1; i> 0; i--) { const j = Math.floor(Math.random() * (i + 1)); [a[i],
    a[j]] = [a[j], a[i]]; } return a; }; var arr = category;shuffle(arr);let selectedCat =arr.slice(0, 5); %>
    <div class="right-section" style="margin-top: 10px;">
        <div class="feed-wrapper">
            <h3 class="feed-card-header">Trending Tags</h3>
            <div class="inner-cmn-paddng">
                <div class="trending-flx-wrap">
                    <% trendingTags.forEach(function(tags) { %>
                    <a href="/tags/<%- tags.m %>">
                        <div class="tg">
                            <div>#<%- tags.m %></div>
                        </div>
                     </a>
                    <% }) %>
                </div>
                <div class="divider">
                    <form action="#" class="side-search">
                        <input type="text" placeholder="Search">
                        <span class="input-icon">
                            <svg viewBox="0 0 487.95 487.95" style="enable-background:new 0 0 487.95 487.95;" xml:space="preserve"><g><g><path d="M481.8,453l-140-140.1c27.6-33.1,44.2-75.4,44.2-121.6C386,85.9,299.5,0.2,193.1,0.2S0,86,0,191.4s86.5,191.1,192.9,191.1c45.2,0,86.8-15.5,119.8-41.4l140.5,140.5c8.2,8.2,20.4,8.2,28.6,0C490,473.4,490,461.2,481.8,453z M41,191.4c0-82.8,68.2-150.1,151.9-150.1s151.9,67.3,151.9,150.1s-68.2,150.1-151.9,150.1S41,274.1,41,191.4z"></path></g></g></svg>
                            </span>
                    </form>
                    <a href="/affiliates" class="access-btn">Invite Friends To Earn</a>
                </div>
            <div class="feed-trending-card" style="margin-top: 20px;">
                <h3 class="feed-card-header style-2">Recomended Topics</h3>
                <% for(let i=0; i < selectedCat.length; i++) { %>
                    <div class="feed-card-trend">
                        <div class="sugg-result-wapper">
                            <div class="search-user-details">
                                <div class="search-user-warp">
                                    <div class="right_catg">
                                        <span class="rside_cat_img"></span>
                                        <a href="/category/<%- selectedCat[i].toLowerCase() %>"><%- selectedCat[i] %>
                                        </a>
                                    </div>
                                    <% if(loguser && loguser !==''){ 
                                        let catgFollowList=JSON.stringify(acct.categoryFollows);
                                        if((catgFollowList) && catgFollowList.includes(selectedCat[i].toLowerCase())) 
                                        {
                                            %><span class="follow-btn-wrap btn_unfollow_catg"
                                            data-catg="<%- selectedCat[i] %>">Following</span><% 
                                        }else{ 
                                            %><span class="follow-btn-wrap btn_follow_catg"
                                                data-catg="<%- selectedCat[i] %>">Follow</span><% 
                                        } 
                                    } else { 
                                        %><span class="follow-btn-wrap btn_follow_catg" data-catg="<% selectedCat[i] %>">Follow</span><% 
                                    } %>
                                </div>
                            </div>
                        </div>
                    </div><% 
                } %>
                <a href="/explore/categories" id="sidebar_categories">
                    <div class="feed-more">Show More Categories</div>
                </a>
            </div>
        </div>
    </div>
